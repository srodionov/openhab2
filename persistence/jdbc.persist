Strategies {
	everyMinute	: "0 * * * * ?"
	every5Minutes 	: "0 */5 * * * ?"
   	everyHour   	: "0 0 * * * ?"
   	everyDay    	: "0 0 0 * * ?"
	default 	= everyChange
}

Items {
	inTemperature*, exTemperature* : strategy = every5Minutes
	Power_Consumption_KWh, Power_Meter, srodionov_location, arodionov_location, AC_mode, AC_fan, AC_v_flow, AC_h_flow, Input_voltage : strategy = everyChange
	restoreState* : strategy = restoreOnStartup
}